<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">Ember Links</a>
    </div>
    <div class="navbar-collapse collapse">
      <ul class="nav navbar-nav">
        {{#link-to "links" tagName="li"}}
          <a {{bind-attr href="view.href"}}>Links</a>
        {{/link-to}}
      </ul>
      <ul class="nav navbar-nav navbar-right">
        {{#if isAuthenticated}}
          {{#link-to "user" tagName="li"}}
            <a {{bind-attr href="view.href"}}>Welcome {{currentUser.fullName}}</a>
          {{/link-to}}
          {{#link-to "sessions.sign_out" tagName="li"}}
            <a {{bind-attr href="view.href"}}>Sign Out</a>
          {{/link-to}}
        {{else}}
        {{#link-to "sign_up" tagName="li"}}
          <a {{bind-attr href="view.href"}}>Sign Up</a>
        {{/link-to}}
          {{#link-to "sessions.sign_in" tagName="li"}}
            <a {{bind-attr href="view.href"}}>Sign In</a>
          {{/link-to}}
        {{/if}}
      </ul>
    </div><!--/.navbar-collapse -->
  </div>
</div>

<div class="container">


{{outlet}}

</div>
